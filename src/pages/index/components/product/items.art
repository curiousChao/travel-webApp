
{{each items}}
<li class="product-item">
    <a href="./details.html">
        <p class="product-pic">
            <img src="{{$value.url}}" alt="" 
            class="product-img" data-number="{{$value.id}}">
        </p>
        <p class="product-name" data-number="{{$value.id}}">
            {{$value.title}}
        </p>
        <p class="product-price" data-number="{{$value.id}}">{{$value.price}}元起</p>
    </a>
</li>
{{/each}}
 